<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Google Suite" parallel="tests">
  <test thread-count="5" name="Testing on Chrome">
	<parameter name="browser" value="Chrome"></parameter>
    <classes>
      <class name="paralleltestingGoogle.TestCase1"/>
      <class name="paralleltestingGoogle.TestCase2"/>
    </classes>
  </test> <!-- Test -->
   <test thread-count="5" name="Testing on Edge">
	<parameter name="browser" value="Edge"></parameter>
    <classes>
      <class name="paralleltestingGoogle.TestCase1"/>
      <class name="paralleltestingGoogle.TestCase2"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
